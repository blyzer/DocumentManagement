@model DocumentManagement.Mvc.Services.MenuManager
<!-- Left side column. contains the logo and sidebar -->
<aside class="main-sidebar">
    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">
        <!-- Sidebar user panel -->
        <div class="user-panel">
            <div class="pull-left image">
                <img src="https://outlook.office365.com/owa/service.svc/s/GetPersonaPhoto?email=@User.Identity.GetUserEmail()&UA=0&size=HR64x64&sc=1468233338850" class="user-image" alt="User Image">
            </div>
            <div class="pull-left info">
                <p>@User.Identity.Name</p>
                <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
            </div>
        </div>
        @Html.Menu(Model.RenderToHtml())
    </section>
</aside>
